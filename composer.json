{
    "name": "ixocreate/asset",
    "description": "Asset for IXOCREATE",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop",
        "ixocreate/project-uri": "dev-develop",
        "symfony/asset": "^4.1",
        "ocramius/package-versions": "^1.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Asset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Asset\\": "tests/misc/",
            "IxocreateTest\\Asset\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
