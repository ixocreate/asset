{
    "name": "ixocreate/asset-package",
    "description": "IXOCREATE Asset Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "ixocreate/application": "dev-develop",
        "ixocreate/template-package": "dev-develop",
        "symfony/asset": "^5.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Asset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Asset\\": "tests/misc/",
            "Ixocreate\\Test\\Asset\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
